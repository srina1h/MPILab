MOV R2, #00H ; 1'S COUNT
MOV R1, #08H
MOV R0, #06H ;HIGHER ORDER BYTE
MOV A, R0
LOOP1:
RRC A
JNC SKIP1
INC R2
SKIP1 :
DJNZ R1, LOOP1
;--------------
MOV R1, #08H
MOV R0, #1FH ; LOWER ORDER BYTE
MOV A, R0
LOOP2:
RRC A
JNC SKIP2
INC R2
SKIP2 :
DJNZ R1, LOOP2
;----------------
CLR C
MOV A, #10H
MOV B, R2
SUBB A, B
MOV 03, A
HERE : SJMP HERE